        </activity>
        <activity android:name=".initWindow" />
        <activity android:name=".LoginWindow" />
        <activity android:name=".createAccount" />
        <activity android:name=".aboutUs" />
        <activity android:name=".Services" />
        <activity android:name=".Sell" />
        <activity android:name=".rent" />
        <activity android:name=".buy" />
        <activity android:name=".selfDriven" />
